<div class='container jumbotron'>
  <h1><%= @artist.id ? ('Edit Artist') : ('Create Artist') %></h1>
  <%= form_for @artist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :rank %>
    <%= f.text_field :rank %>
    <%= f.label :soundcloud_id %>
    <%= f.text_field :soundcloud_id %>
    <%= f.label :homepage %>
    <%= f.text_field :homepage %>
    <%= f.submit %>
  <% end %>
</div>
